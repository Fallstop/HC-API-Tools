{
  "name": "node",
  "version": "0.0.1",
  "private": true,
  "license": "mit-license",
  "scripts": {
    "run": "node ./dist-server/bin/www.js",
    "start": "npm-run-all build run",
    "start:dev": "NODE_ENV=development yarn run start",
    "start:prod": "NODE_ENV=production yarn run start",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "clean:full": "rimraf dist-server node_modules",
    "build": "npm-run-all clean transpile",
    "dev": "nodemon"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "nodemonConfig": {
    "exec": "yarn run start:dev",
    "watch": [
      "server/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "dependencies": {
    "core-js": "^3.10.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "googleapis": "^70.0.0",
    "morgan": "~1.10.0",
    "npm-run-all": "^4.1.5",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}

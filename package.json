{
  "name": "node",
  "version": "0.0.1",
  "private": true,
  "license": "mit-license",
  "scripts": {
    "run": "node dist-server/bin/www.js",
    "start": "yarn run start:prod",
    "start:dev": "NODE_ENV=development ts-node server/bin/www.ts",
    "start:prod": "NODE_ENV=production  npm-run-all transpile run",
    "transpile": "./node_modules/typescript/bin/tsc",
    "clean": "rimraf dist-server",
    "clean:full": "rimraf dist-server node_modules",
    "copy-assets": "cp -r server/views dist-server/",
    "build": "npm-run-all transpile copy-assets",
    "dev": "nodemon"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-env"
    ]
  },
  "nodemonConfig": {
    "exec": "yarn run start:dev",
    "watch": [
      "server/*"
    ],
    "ext": "js,ts,ejs",
    "ignore": [
      "**/__tests__/**",
      "*.test.ts",
      "*.spec.ts"
    ]
  },
  "dependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/express": "^4.17.11",
    "@types/gapi.calendar": "^3.0.4",
    "@types/node": "^14.14.37",
    "cors": "^2.8.5",
    "date-fns": "^2.19.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "~4.17.1",
    "googleapis": "^70.0.0",
    "morgan": "~1.10.0",
    "npm-run-all": "^4.1.5",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1"
  }
}
